# ==============================================
# POS Frontend Environment Variables Template
# ==============================================
# Copy this file to .env for local development
# On Render, set these in the Environment section
# NEVER commit .env to version control!

# ==============================================
# Backend API Configuration
# ==============================================
# Local development
VITE_API_URL=http://localhost:5000

# Production (update with your Render backend URL)
# VITE_API_URL=https://your-backend-name.onrender.com

# ==============================================
# Application Configuration
# ==============================================
VITE_APP_NAME=Zosh POS
VITE_APP_VERSION=1.0.0

# ==============================================
# Feature Flags
# ==============================================
VITE_ENABLE_ANALYTICS=false
VITE_ENABLE_DEBUG=false

# ==============================================
# Payment Gateway (Frontend Keys)
# ==============================================
# Only use public/publishable keys on frontend
# VITE_RAZORPAY_KEY_ID=your_razorpay_key_id_here
# VITE_STRIPE_PUBLISHABLE_KEY=your_stripe_publishable_key_here

# ==============================================
# Third-party Services
# ==============================================
# VITE_GOOGLE_ANALYTICS_ID=your_ga_id_here
# VITE_SENTRY_DSN=your_sentry_dsn_here

# ==============================================
# Development Only
# ==============================================
# Enable React DevTools in production (not recommended)
# VITE_ENABLE_DEVTOOLS=false

